<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
	<SolidColorBrush x:Key="Border/Chart" Color="{DynamicResource Stroke/Medium/Color}" />
	<SolidColorBrush x:Key="Border/Chart/DrawingArea" Color="{DynamicResource Stroke/Light/Color}" />
    <SolidColorBrush x:Key="Border/CrosshairSeriesLabel" Color="{DynamicResource Stroke/Medium/Color}" />
	<SolidColorBrush x:Key="Fill/Chart" Color="{DynamicResource Background/Chart/Color}" />
	<SolidColorBrush x:Key="Fill/Chart/DrawingArea" Color="{DynamicResource Background/DrawingArea/Color}" />
    <SolidColorBrush x:Key="Fill/CrosshairSeriesLabel" Color="{DynamicResource Background/Chart/Color}" />
	
	<Thickness x:Key="ChartBorderThickness">1</Thickness>
    <Thickness x:Key="CrosshairSeriesLabelBorderThickness">1</Thickness>
	<Thickness x:Key="DrawingAreaBorderThickness">1</Thickness>
	
	<CornerRadius x:Key="ChartCornerRadius">6</CornerRadius>
    <CornerRadius x:Key="CrosshairSeriesLabelCornerRadius">6</CornerRadius>
    <CornerRadius x:Key="SeriesColorMarker">6</CornerRadius>
    
    <sys:Double x:Key="SeriesMarkerSize">21</sys:Double>
	
	<Thickness x:Key="Padding/Chart">8</Thickness>
</ResourceDictionary>
