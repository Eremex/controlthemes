<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="Border/Chart" Color="{DynamicResource Stroke/Medium/Color}" />
	<SolidColorBrush x:Key="Border/Chart/DrawingArea" Color="{DynamicResource Stroke/Light/Color}" />
	<SolidColorBrush x:Key="Fill/Chart" Color="{DynamicResource Background/Chart/Color}" />
	<SolidColorBrush x:Key="Fill/Chart/DrawingArea" Color="{DynamicResource Background/DrawingArea/Color}" />
	
	<Thickness x:Key="ChartBorderThickness">1</Thickness>
	<Thickness x:Key="DrawingAreaBorderThickness">1</Thickness>
	
	<CornerRadius x:Key="ChartCornerRadius">6</CornerRadius>
	
	<Thickness x:Key="Padding/Chart">8</Thickness>
</ResourceDictionary>
